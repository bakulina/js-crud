image.src = "https://img.icons8.com/ios/40/ffffff/sms.png";
    var icons = document.querySelectorAll('.webs__list a');
    for(var i = 0, len = icons.length; i < len; i++) {
      icons[i].classList.remove('animated--icons');
    }
}
  
}
